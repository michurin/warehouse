{
  "stats": {},
  "api": {
    "tag": "api",
    "services": [
      "ReflectionService",
      "StatsService"
    ]
  },
  "policy": {
    "levels": {
      "0": {
        "statsUserUplink": true,
        "statsUserDownlink": true
      }
    },
    "system": {
      "statsInboundUplink": true,
      "statsInboundDownlink": true,
      "statsOutboundUplink": true,
      "statsOutboundDownlink": true
    }
  },
  "log": {
    "loglevel": "warning"
  },
  "inbounds": [
    {
      "tag": "vpn",
      "port": 443,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "email": "first@x.com",
            "id": "${{UUID_1}}",
            "level": 0,
            "flow": "xtls-rprx-vision"
          },
          {
            "email": "second@x.com",
            "id": "${{UUID_2}}",
            "level": 0,
            "flow": "xtls-rprx-vision"
          },
          {
            "email": "mary@x.com",
            "id": "${{UUID_3}}",
            "level": 0,
            "flow": "xtls-rprx-vision"
          }
        ],
        "decryption": "none"
      },
      "streamSettings": {
        "network": "tcp",
        "security": "reality",
        "realitySettings": {
          "dest": "google.com:443",
          "serverNames": [
            "google.com",
            "www.google.com"
          ],
          "privateKey": "${{KEY}}",
          "shortIds": [
            "aaaaaaaa",
            "bbbbbbbb",
            "cccccccc"
          ]
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ]
      }
    },
    {
      "tag": "api",
      "listen": "127.0.0.1",
      "port": 5432,
      "protocol": "dokodemo-door",
      "settings": {
        "address": "127.0.0.1"
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "tag": "direct"
    }
  ],
  "routing": {
    "rules": [
      {
        "inboundTag": [
          "api"
        ],
        "outboundTag": "api",
        "type": "field"
      }
    ]
  }
}
