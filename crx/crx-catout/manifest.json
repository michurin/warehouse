{
  "name": "XCutOut",
  "version": "1.25.11",
  "description": "Cut out annoying unwanted elements, highlight useful elements",
  "manifest_version": 3,
  "content_scripts": [
    {"matches": ["https://mail.yandex.ru/*"], "js": ["m.js"]},
    {"matches": ["https://yandex.ru/*"], "js": ["ym.js", "p.js"]},
    {"matches": ["https://context.reverso.net/*"], "js": ["c.js"]},
    {"matches": ["https://dictionary.cambridge.org/*"], "js": ["d.js"]},
    {"matches": ["https://www.openstreetmap.org/*"], "js": ["osm.js"]},
    {"matches": ["https://www.radarbox.com/*"], "js": ["rb.js"]},
    {"matches": ["https://*.zoom.us/j/*"], "js": ["z.js"]},
    {"matches": ["https://*.zoom.us/saml/*"], "js": ["z.js"]},
    {"matches": ["https://*.slack.com/archives/*"], "js": ["z.js"]},
    {"matches": ["https://go.dev/play/*"], "js": ["go.js"]},
    {"matches": ["https://www.google.com/search?*"], "js": ["google.js"]},
    {"matches": ["https://stackoverflow.com/*", "https://*.stackexchange.com/*"], "js": ["sof.js"]},

    {"matches": ["https://jr.avito.ru/*"], "js": ["av.js"]},
    {"matches": ["https://cf.avito.ru/*"], "js": ["cf.js"]},
    {"matches": ["https://exchange.avito.ru/*"], "js": ["avex.js"]},
    {"matches": ["https://mail.avito.ru/*"], "js": ["avex.js"]},
    {"matches": ["https://people.avito.ru/users/*"], "js": ["ppl.js"]},

    {"matches": ["https://github.com/inDriver/*/pulls"], "js": ["idprs.js"]}
  ]
}
